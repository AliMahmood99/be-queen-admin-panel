# BE QUEEN Admin Panel - Production Environment Variables
# Copy this file and configure in Vercel Dashboard → Settings → Environment Variables

# API Configuration (REQUIRED)
# Replace with your production API URL
VITE_API_BASE_URL=https://api.bequeen.qa/api
VITE_API_TIMEOUT=30000

# Application (REQUIRED)
VITE_APP_NAME="BE QUEEN Admin Panel"
VITE_APP_VERSION=1.0.0

# Feature Flags (REQUIRED)
# Set to 'false' for production to use real API
VITE_ENABLE_MOCK_DATA=false
# Set to 'false' for production
VITE_ENABLE_DEBUG=false
